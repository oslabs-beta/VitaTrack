{
  "name": "vitrack-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend server for VitaTrack health tracker",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec \"node --loader ts-node/esm server.ts\"",
    "start": "concurrently \"npm run server\" \"npm run frontend\" \"npm run open-tabs\"",
    "server": "node dist/server.js & sleep 1 && open-cli http://localhost:5001",
    "frontend": "npm run dev --prefix frontend",
    "build": "cd tableviewer && npm run build && cd .. && tsc -p tsconfig.server.json",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^6.1.0",
    "pg": "^8.11.5",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.0",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "open-cli": "^8.0.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
